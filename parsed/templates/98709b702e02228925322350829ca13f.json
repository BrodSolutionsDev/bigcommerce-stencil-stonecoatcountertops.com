{"components/common/paginator":"<nav class=\"pagination\" aria-label=\"pagination\">\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use href=\"#icon-chevron-left\"></use>\n                        </svg>\n                    </i>\n                    {{lang 'common.previous'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}\n                   aria-current=\"page\"\n                   data-pagination-current-page-link\n                   {{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    {{lang 'common.next'}}\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use href=\"#icon-chevron-right\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n","components/custom/deep-pour-calculator":"\r\n  <div class=\"calculator-section-heading\">\r\n  </div>\r\n<div class=\"calculator-section-coverage\">\r\n    {{#each fields}}\r\n      {{#if name '==' '1 Gallon Kit'}}\r\n        <div class=\"kit-size\">\r\n          <img src=\"{{cdn 'img/info-icons/one_gallon_bucket.png'}}\" alt=\"One gallon kit\">\r\n          <!-- <span class=\"equals\">=</span> -->\r\n          <img src=\"{{cdn 'img/info-icons/square_footage.png'}}\" alt=\"Square footage icon\">\r\n          <div class=\"covers\">\r\n            <p class=\"value\">{{value}}</p>\r\n            <p>Sq Ft</p>\r\n          </div>\r\n        </div>\r\n      {{/if}}\r\n      {{#if name '==' '1/2 Gallon Kit'}}\r\n        <div class=\"kit-size\">\r\n          <img src=\"{{cdn 'img/info-icons/half_gallon_bucket.png'}}\" alt=\"Half gallon kit\">\r\n          <!-- <span class=\"equals\">=</span> -->\r\n          <img src=\"{{cdn 'img/info-icons/square_footage.png'}}\" alt=\"Square footage icon\">\r\n          <div class=\"covers\">\r\n            <p class=\"value\">{{value}}</p>\r\n            <p>Sq Ft</p>\r\n          </div>\r\n        </div>\r\n      {{/if}}\r\n    {{/each}}\r\n<div class=\"epoxy-calculator\">\r\n    <h4>Volume Calculator</h4>\r\n    <!-- length x width inputs -->\r\n    <div class=\"epoxy-calculator-inputs flex-row\">\r\n        <div class=\"epoxy-calculator-input\">\r\n            <label for=\"length\">Length (inches)</label>\r\n            <input type=\"number\" id=\"deepPourLength\" name=\"length\" placeholder=\"inches\" />\r\n        </div>\r\n        <div class=\"epoxy-calculator-input\">\r\n            <label for=\"width\">Width (inches)</label>\r\n            <input type=\"number\" id=\"deepPourWidth\" name=\"width\" placeholder=\"inches\" />\r\n        </div>\r\n        <div class=\"epoxy-calculator-input\">\r\n            <label for=\"depth\">Depth (inches)</label>\r\n            <input type=\"number\" id=\"deepPourDepth\" name=\"depth\" placeholder=\"inches\">\r\n        </div>\r\n        <!-- submit and calculate -->\r\n        <div class=\"epoxy-calculator-submit\">\r\n            <button class=\"scc-button-action button button--small\" id=\"deepPourCalculate\">Calculate</button>\r\n        </div>\r\n        </div>\r\n        \r\n        <!-- results -->\r\n        <div class=\"epoxy-calculator-results flex-row\">\r\n            <div class=\"epoxy-calculator-results-amount\">\r\n                <h5>US Fluid Ounces Needed: <span id=\"results-deep-pour-square-feet\">0</span></h5>\r\n            </div>\r\n            <div class=\"epoxy-calculator-results-amount\">\r\n                <h5>Gallons Needed: <span id=\"results-deep-pour-gallons\">0</span></h5>\r\n            </div>\r\n            <h5><em>Length (in) &times; Width (in) &times; Depth (in) &divide; 231 = Volume (Gallons)</em></h5>\r\n        </div>\r\n        <script>\r\n            // get inputs\r\n            const deepPourLength = document.getElementById('deepPourLength');\r\n            const deepPourWidth = document.getElementById('deepPourWidth');\r\n            const deepPourDepth = document.getElementById('deepPourDepth');\r\n            const deepPourCalculate = document.getElementById('deepPourCalculate');\r\n            // get results\r\n            const resultsDeepPourSquareFeet = document.getElementById('results-deep-pour-square-feet');\r\n            const resultsDeepPourGallons = document.getElementById('results-deep-pour-gallons');\r\n            // calculate square feet\r\n            function calculateDeepPourSquareFeet() {\r\n                const cubicInches = deepPourLength.value* deepPourWidth.value * deepPourDepth.value;\r\n                const ounces = cubicInches * 0.554;\r\n                resultsDeepPourSquareFeet.innerHTML = ounces.toFixed(2);\r\n            }\r\n            // calculate gallons\r\n            function calculateDeepPourGallons() {\r\n                //const squareFeet = deepPourLength.value * deepPourWidth.value * (deepPourDepth.value / 12);\r\n                //const gallons = squareFeet / 20;\r\n                //resultsDeepPourGallons.innerHTML = gallons.toFixed(2);\r\n                // Tanner quick edits\r\n                const cubicInches = deepPourLength.value * deepPourWidth.value * deepPourDepth.value;\r\n                const gallons = cubicInches / 231;\r\n                resultsDeepPourGallons.innerHTML = gallons.toFixed(2);\r\n            }\r\n            // calculate square feet and gallons\r\n            function calculateDeepPourAll() {\r\n                calculateDeepPourSquareFeet();\r\n                calculateDeepPourGallons();\r\n            }\r\n            // calculate on click\r\n            deepPourCalculate.addEventListener('click', calculateDeepPourAll);\r\n        </script>\r\n\r\n</div>\r\n</div>\r\n\r\n","components/custom/epoxy-calculator":"\n  <div class=\"calculator-section-heading\">\n  </div>\n<div class=\"calculator-section-coverage\">\n    {{#each fields}}\n      {{#if name '==' '1 Gallon Kit'}}\n        <div class=\"kit-size\">\n          <img src=\"{{cdn 'img/info-icons/one_gallon_bucket.png'}}\" alt=\"One gallon kit\">\n          <!-- <span class=\"equals\">=</span> -->\n          <img src=\"{{cdn 'img/info-icons/square_footage.png'}}\" alt=\"Square footage icon\">\n          <div class=\"covers\">\n            <p class=\"value\">{{value}}</p>\n            <p>Sq Ft</p>\n          </div>\n        </div>\n      {{/if}}\n      {{#if name '==' '1/2 Gallon Kit'}}\n        <div class=\"kit-size\">\n          <img src=\"{{cdn 'img/info-icons/half_gallon_bucket.png'}}\" alt=\"Half gallon kit\">\n          <!-- <span class=\"equals\">=</span> -->\n          <img src=\"{{cdn 'img/info-icons/square_footage.png'}}\" alt=\"Square footage icon\">\n          <div class=\"covers\">\n            <p class=\"value\">{{value}}</p>\n            <p>Sq Ft</p>\n          </div>\n        </div>\n      {{/if}}\n    {{/each}}\n<div class=\"epoxy-calculator\">\n    <h4>Square Foot Calculator</h4>\n    <!-- length x width inputs -->\n    <div class=\"epoxy-calculator-inputs flex-row\">\n        <div class=\"epoxy-calculator-input\">\n            <label for=\"length\">Length (feet)</label>\n            <input type=\"number\" id=\"length\" name=\"length\" placeholder=\"feet\" />\n        </div>\n        <div class=\"epoxy-calculator-input\">\n            <label for=\"width\">Width (feet)</label>\n            <input type=\"number\" id=\"width\" name=\"width\" placeholder=\"feet\" />\n        </div>\n        <!-- submit and calculate -->\n        <div class=\"epoxy-calculator-submit\">\n            <button class=\"scc-button-action button button--small\" id=\"calculate\">Calculate</button>\n        </div>\n        </div>\n        \n        <!-- results -->\n        <div class=\"epoxy-calculator-results flex-row\">\n            <div class=\"epoxy-calculator-results-amount\">\n                <h5>Square Feet: <span id=\"results-square-feet\">0</span></h5>\n            </div>\n            <div class=\"epoxy-calculator-results-amount\">\n                <h5>Gallons Needed: <span id=\"results-gallons\">0</span></h5>\n            </div>\n            <h5><em>Length &times; Width &divide; 20 = Gallons</em></h5>\n        </div>\n        <script>\n            // get inputs\n            const length = document.getElementById('length');\n            const width = document.getElementById('width');\n            const calculate = document.getElementById('calculate');\n            // get results\n            const resultsSquareFeet = document.getElementById('results-square-feet');\n            const resultsGallons = document.getElementById('results-gallons');\n            // calculate square feet\n            function calculateSquareFeet() {\n                const squareFeet = length.value * width.value;\n                resultsSquareFeet.innerHTML = squareFeet;\n            }\n            // calculate gallons\n            function calculateGallons() {\n                const squareFeet = length.value * width.value;\n                const gallons = squareFeet / 20;\n                resultsGallons.innerHTML = gallons.toFixed(2);\n            }\n            // calculate square feet and gallons\n            function calculateAll() {\n                calculateSquareFeet();\n                calculateGallons();\n            }\n            // calculate on click\n            calculate.addEventListener('click', calculateAll);\n        </script>\n\n</div>\n</div>\n\n","components/elevate/products-description-tabs":"<ul class=\"tabs\" data-tab>\r\n    <li class=\"tab is-active\">\r\n        <a class=\"tab-title\" href=\"#tab-description\">\r\n          <span class=\"icon-Description show theme-icon\" data-icon=\"&#xe90a;\"></span>\r\n          <span class=\"show\">{{lang 'products.description'}}</span>\r\n\r\n          \r\n        \r\n        </a>\r\n    </li> \r\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-{{dashcase (lowercase (sanitize theme_settings.pdp-custom-fields-tab-label))}}\">\r\n              <span class=\"icon-brand show theme-icon\" data-icon=\"&#xe909;\"></span>\r\n              <span class=\"show\">{{sanitize theme_settings.pdp-custom-fields-tab-label}}</span>\r\n            </a>\r\n        </li> \r\n    {{/all}}\r\n    <!-- {{#if product.warranty}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-warranty\">\r\n              <span class=\"icon-Specifications show theme-icon\" data-icon=\"&#xe90c;\"></span>\r\n              <span class=\"show\">{{lang 'products.warranty'}}</span>\r\n            </a>\r\n        </li>\r\n    {{/if}} -->\r\n    {{#all settings.show_product_reviews theme_settings.show_product_reviews_tabs product.reviews.total}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title productView-reviewTabLink\" href=\"#tab-reviews\">\r\n              <span class=\"icon-Reviews show theme-icon\" data-icon=\"&#xe90b;\"></span>\r\n              <span class=\"show\">{{lang 'products.reviews.title'}}</span>\r\n            </a>\r\n        </li>\r\n    {{/all}}\r\n    <!-- {{#if product.videos.list.length}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-videos\">\r\n              <span class=\"icon-video show theme-icon\" data-icon=\"&#xe90d;\"></span>\r\n              <span class=\"show\">{{lang 'products.videos.header'}}</span>\r\n            </a>\r\n        </li>\r\n    {{/if}} -->\r\n    <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-training\">\r\n              <span class=\"icon-video show theme-icon\" data-icon=\"&#xe90d;\"></span>\r\n              <span class=\"show\">Training</span>\r\n            </a>\r\n        </li>\r\n    <!-- calculator tab -->\r\n    <li class=\"tab\">\r\n        <a class=\"tab-title\" href=\"#tab-calculator\">\r\n          <span class=\"icon-calculator show theme-icon\"><img height=\"22\" width=\"22\" src=\"{{cdn 'img/calculator.png'}}\" alt=\"calculator icon\"></span>\r\n          <span class=\"show\">Calculator</span>\r\n        </a>\r\n    </li>\r\n    <!-- answerbase tab -->\r\n    <li class=\"tab\">\r\n      <a class=\"tab-title\" href=\"#tab-answerbase\">\r\n        <span class=\"icon-calculator show theme-icon\"><img height=\"22\" width=\"22\" src=\"{{cdn 'img/q&a.png'}}\" alt=\"q&a icon\"></span>\r\n        <span class=\"show\">Q&A</span>\r\n      </a>\r\n  </li>\r\n</ul>\r\n<div class=\"tabs-contents\">\r\n    <div class=\"tab-content is-active\" id=\"tab-description\">\r\n      <div class=\"description-body-inner\">{{{product.description}}}</div>\r\n      <button id=\"show-more-btn\" class=\"button button--primary\">Show more</button>\r\n\r\n      <script>\r\n        const showMoreBtn = document.getElementById(\"show-more-btn\");\r\n        const descriptionBody = document.querySelector(\".description-body-inner\");\r\n    \r\n        if (descriptionBody.getBoundingClientRect().height > 300) {\r\n            descriptionBody.style.height = \"300px\";\r\n            showMoreBtn.style.display = \"block\";\r\n            descriptionBody.classList.add(\"showMoreBlur\");\r\n    \r\n            showMoreBtn.addEventListener(\"click\", function() {\r\n                if (descriptionBody.getBoundingClientRect().height <= 300) {\r\n                    descriptionBody.style.height = \"fit-content\";\r\n                    showMoreBtn.innerText = \"Show less\"\r\n                    descriptionBody.classList.remove(\"showMoreBlur\")\r\n                } else {\r\n                    descriptionBody.style.height = \"300px\";\r\n                    showMoreBtn.innerText = \"Show more\";\r\n                    descriptionBody.classList.add(\"showMoreBlur\")\r\n                }\r\n            });\r\n        } else {\r\n          showMoreBtn.setAttribute(\"style\", \"display: none !important;\");\r\n        }\r\n      </script>\r\n    </div>\r\n    \r\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\r\n      <div class=\"tab-content\" id=\"tab-{{dashcase (lowercase (sanitize theme_settings.pdp-custom-fields-tab-label))}}\">\r\n         \r\n            {{#each product.custom_fields}}\r\n            <dl class=\"productView-info-dl\">\r\n               <dt class=\"productView-info-name\">{{name}}:</dt>\r\n               <dd class=\"productView-info-value\">{{{value}}}</dd>\r\n            </dl>\r\n            {{/each}}\r\n         \r\n      </div>\r\n   {{/all}}\r\n   <!-- {{#if product.warranty}}\r\n       <div class=\"tab-content\" id=\"tab-warranty\">\r\n           {{{product.warranty}}}\r\n       </div>\r\n   {{/if}} -->\r\n   {{#all settings.show_product_reviews theme_settings.show_product_reviews_tabs}}\r\n       <div class=\"tab-content\" id=\"tab-reviews\">\r\n           {{> components/products/reviews reviews=product.reviews product=product urls=urls}}\r\n       </div>\r\n   {{/all}}\r\n   <!-- {{#if product.videos.list.length}}\r\n      <div class=\"tab-content\" id=\"tab-videos\">\r\n        {{> components/elevate/videos product.videos}}\r\n      </div>\r\n    {{/if}} -->\r\n    <div class=\"tab-content\" id=\"tab-training\">\r\n      {{#if product.warranty}}\r\n        <h2>Instructions</h2>\r\n        {{{product.warranty}}}\r\n      {{/if}}\r\n      {{#if product.videos.list.length}}\r\n        <h2>Videos</h2>\r\n        {{> components/elevate/videos product.videos}}\r\n      {{/if}}\r\n    </div>\r\n    <div class=\"tab-content\" id=\"tab-calculator\">\r\n        <h2>Surface Calculator</h2>\r\n        {{> components/custom/epoxy-calculator}}\r\n\r\n        <h2>Deep Pour Calculator</h2>\r\n        {{> components/custom/deep-pour-calculator}}\r\n      </div>\r\n      <div class=\"tab-content\" id=\"tab-answerbase\">\r\n        <div class=\"answerbase-tab\"></div>\r\n      </div>\r\n</div>\r\n","components/elevate/videos":"<section class=\"toggle videoGallery\" data-video-gallery>\r\n    <div class=\"toggle-content is-open\" id=\"videoGallery-content\">\r\n        <div class=\"videoGallery-main\">\r\n            <iframe\r\n                id=\"player\"\r\n                class=\"lazyload\"\r\n                type=\"text/html\"\r\n                width=\"640\"\r\n                height=\"390\"\r\n                frameborder=\"0\"\r\n                webkitAllowFullScreen\r\n                mozallowfullscreen\r\n                allowFullScreen\r\n                data-src=\"//www.youtube.com/embed/{{this.featured.id}}\"\r\n                data-video-player>\r\n            </iframe>\r\n        </div>\r\n        <ul class=\"videoGallery-list\">\r\n        {{#each this.list}}\r\n            <li class=\"videoGallery-item\">\r\n                <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\r\n                    <div class=\"video-figure\">\r\n                        <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\r\n                    </div>\r\n                    <div class=\"video-body\">\r\n                        <h5 class=\"video-title\">{{title_long}}</h5>\r\n                        <p class=\"video-description\">{{description_short}}</p>\r\n                    </div>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n        </ul>\r\n    </div>\r\n\r\n</section>\r\n","components/products/ratings":"{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n","components/products/reviews":"{{#if reviews.total}}\n<section class=\"toggle productReviews\" id=\"product-reviews\" data-product-reviews>\n    <h4 class=\"toggle-title\">\n        {{lang 'products.reviews.header' total=reviews.total}}\n        {{#if theme_settings.show_product_reviews}}\n            <a class=\"toggleLink is-open\" data-collapsible href=\"#productReviews-content\">\n                <span class=\"toggleLink-text toggleLink-text--on\">\n                    {{lang 'products.reviews.hide'}}\n                </span>\n                <span class=\"toggleLink-text toggleLink-text--off\">\n                    {{lang 'products.reviews.show'}}\n                </span>\n            </a>\n        {{/if}}\n    </h4>\n    <div class=\"toggle-content is-open\" id=\"productReviews-content\" aria-hidden=\"false\">\n        <ul class=\"productReviews-list\" id=\"productReviews-list\">\n            {{#each reviews.list}}\n            <li class=\"productReview\">\n                <article>\n                    <header>\n                        <span class=\"productReview-rating rating--small\">\n                            {{> components/products/ratings rating=rating}}\n                            <span class=\"productReview-ratingNumber\">{{ rating }}</span>\n                        </span>\n                        <h5 class=\"productReview-title\">{{{ sanitize title }}}</h5>\n                        {{#if name}}\n                            <p class=\"productReview-author\">\n                                {{{lang 'products.reviews.post_on_by' date=date name=(sanitize name) }}}\n                            </p>\n                        {{else}}\n                            <p class=\"productReview-author\">\n                                {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\n                            </p>\n                        {{/if}}\n                    </header>\n                    <p class=\"productReview-body\">{{{ sanitize text }}}</p>\n                </article>\n            </li>\n            {{/each}}\n        </ul>\n        {{> components/common/paginator pagination.reviews}}\n    </div>\n</section>\n{{/if}}\n"}
